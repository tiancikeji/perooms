						<select class="sel-search" name="" id="sel-rooms">
						  <option value="">1</option>
						  <option value="">2</option>
						</select>
					</p>
				</div>	
					
				<div class="search-item fl">
					<label for="sel-adults">adults</label>
					<p>
						<select class="sel-search" name="" id="sel-adults">
						  <option value="">1</option>
						  <option value="">2</option>
						</select>
					</p>
				</div>
				
				<div class="search-item fl">
					<label for="sel-children">children</label>
					<p>
						<select class="sel-search" name="" id="sel-children">
						  <option value="">1</option>
						  <option value="">2</option>
						</select>
					</p>
				</div>
			</div>
			
			<div class="mt20 tr clearfix">
				<input class="btn btn-large" type="submit" title="Search" value="Search">
			</div>
		</form>
	</div>
	<!--/.search-home -->
	
	<div class="aboutus-home">
		<h2>Why Choose Us?</h2>
		<ul>
			<li>
